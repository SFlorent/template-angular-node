{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "nodemon --exec node --es-module-specifier-resolution=node ./src/server.js --",
    "babel-node": "babel-node",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js ./tests"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.js",
      "**/*.spec.js",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@faker-js/faker": "^7.4.0",
    "compose-middleware": "^5.0.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "got": "^12.3.1",
    "helmet": "^5.1.1",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.8",
    "pdfkit": "^0.13.0",
    "rotating-file-stream": "^3.0.4",
    "sequelize": "^6.21.4",
    "sequelize-auto": "^0.8.8",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "regenerator-runtime": "^0.13.9",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.4"
  }
}
